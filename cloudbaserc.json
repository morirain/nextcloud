{
  "version": "2.0",
  "envId": "{{env.ENV_ID}}",
  "framework": {
    "name": "seafile",
    "plugins": {
      "client": {
        "use": "@cloudbase/framework-plugin-container",
        "inputs": {
          "cpu": 0.5,
          "mem": 1,
          "serviceName": "seafile",
          "servicePath": "/",
          "uploadType": "image",
          "containerPort": 80,
          "imageInfo": {
            "imageUrl": "ccr.ccs.tencentyun.com/tcb-100004082552-tlfc/nextcloud-4gsl68k686c12ad1_kodbox:latest"
          },
          "volumeMounts": {
            "/seafile": "seafile-cfs"
          }
        }
      }
    },
    "requirement": {
      "addons": [
        {
          "type": "CFS",
          "name": "seafile-cfs"
        }
      ]
    }
  }
}
