{
  "version": "2.0",
  "envId": "{{env.ENV_ID}}",
  "framework": {
    "name": "cloudreve",
    "plugins": {
      "client": {
        "use": "@cloudbase/framework-plugin-container",
        "inputs": {
          "cpu": 0.5,
          "mem": 1,
          "serviceName": "cloudreve",
          "servicePath": "/",
          "uploadType": "image",
          "containerPort": 5212,
          "imageInfo": {
            "imageUrl": "xaiverniu/cloudreve:latest"
          },
          "volumeMounts": {
            "/cloudreve": "nextcloud-cfs",
            "/downloads": "nextcloud-cfs"
          }
        }
      }
    },
    "requirement": {
      "addons": [
        {
          "type": "CFS",
          "name": "nextcloud-cfs"
        },
      ]
    }
  }
}
